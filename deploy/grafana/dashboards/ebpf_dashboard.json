{
  "dashboard": {
    "id": 123456,
    "title": "x0tta6bl4 eBPF Performance Dashboard",
    "tags": ["eBPF", "Performance", "Mesh Network"],
    "style": "dark",
    "timezone": "browser",
    "refresh": "10s",
    "editable": true,
    "panels": [
      {
        "id": 1,
        "title": "eBPF Programs Loaded",
        "type": "stat",
        "gridPos": {
          "h": 4,
          "w": 6,
          "x": 0,
          "y": 0
        },
        "targets": [
          {
            "expr": "count(ebpf_health_loader{status=\"healthy\"})",
            "refId": "A"
          }
        ],
        "options": {
          "colorMode": "value",
          "graphMode": "area",
          "justifyMode": "auto",
          "orientation": "auto"
        },
        "thresholds": {
          "steps": [
            {
              "color": "green",
              "value": null
            },
            {
              "color": "orange",
              "value": 0.5
            },
            {
              "color": "red",
              "value": 0.8
            }
          ]
        }
      },
      {
        "id": 2,
        "title": "Total eBPF Packets",
        "type": "stat",
        "gridPos": {
          "h": 4,
          "w": 6,
          "x": 6,
          "y": 0
        },
        "targets": [
          {
            "expr": "sum(ebpf_xdp_counter_tcp_packets + ebpf_xdp_counter_udp_packets + ebpf_xdp_counter_icmp_packets + ebpf_xdp_counter_other_packets)",
            "refId": "A"
          }
        ],
        "options": {
          "colorMode": "value",
          "graphMode": "area",
          "justifyMode": "auto",
          "orientation": "auto"
        },
        "thresholds": {
          "steps": [
            {
              "color": "green",
              "value": null
            },
            {
              "color": "orange",
              "value": 100000
            },
            {
              "color": "red",
              "value": 1000000
            }
          ]
        }
      },
      {
        "id": 3,
        "title": "eBPF Health Status",
        "type": "stat",
        "gridPos": {
          "h": 4,
          "w": 6,
          "x": 12,
          "y": 0
        },
        "targets": [
          {
            "expr": "ebpf_health_total",
            "refId": "A"
          }
        ],
        "options": {
          "colorMode": "value",
          "graphMode": "area",
          "justifyMode": "auto",
          "orientation": "auto"
        },
        "thresholds": {
          "steps": [
            {
              "color": "green",
              "value": null
            },
            {
              "color": "orange",
              "value": 3
            },
            {
              "color": "red",
              "value": 6
            }
          ]
        }
      },
      {
        "id": 4,
        "title": "eBPF Errors",
        "type": "stat",
        "gridPos": {
          "h": 4,
          "w": 6,
          "x": 18,
          "y": 0
        },
        "targets": [
          {
            "expr": "sum(ebpf_health_unhealthy)",
            "refId": "A"
          }
        ],
        "options": {
          "colorMode": "value",
          "graphMode": "area",
          "justifyMode": "auto",
          "orientation": "auto"
        },
        "thresholds": {
          "steps": [
            {
              "color": "green",
