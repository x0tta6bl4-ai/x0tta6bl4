            },
            {
              "color": "red",
              "value": 0.3
            }
          ]
        }
      },
      {
        "id": 5,
        "title": "Packets by Protocol",
        "type": "graph",
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 4
        },
        "targets": [
          {
            "expr": "ebpf_xdp_counter_tcp_packets",
            "legendFormat": "TCP",
            "refId": "A"
          },
          {
            "expr": "ebpf_xdp_counter_udp_packets",
            "legendFormat": "UDP",
            "refId": "B"
          },
          {
            "expr": "ebpf_xdp_counter_icmp_packets",
            "legendFormat": "ICMP",
            "refId": "C"
          },
          {
            "expr": "ebpf_xdp_counter_other_packets",
            "legendFormat": "Other",
            "refId": "D"
          }
        ],
        "yaxes": [
          {
            "format": "short",
            "label": "Packets",
            "logBase": 1,
            "max": null,
            "min": 0
          }
        ],
        "options": {
          "tooltip": {
            "mode": "single"
          },
          "legend": {
            "showLegend": true
          }
        }
      },
      {
        "id": 6,
        "title": "Interface Health",
        "type": "gauge",
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 4
        },
        "targets": [
          {
            "expr": "sum(ebpf_interface_health{status=\"healthy\"}) / count(ebpf_interface_health)",
            "refId": "A"
          }
        ],
        "maxValue": 1,
        "minValue": 0,
        "thresholds": [
          {
            "color": "green",
            "value": null
          },
          {
            "color": "yellow",
            "value": 0.8
          },
          {
            "color": "red",
            "value": 0.5
          }
        ],
        "unit": "percent"
      },
      {
        "id": 7,
        "title": "Flow Statistics",
        "type": "graph",
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 12
        },
        "targets": [
          {
            "expr": "sum(ebpf_flow_bytes)",
            "legendFormat": "Total Bytes",
            "refId": "A"
          },
          {
            "expr": "sum(ebpf_flow_packets)",
            "legendFormat": "Total Packets",
            "refId": "B"
          },
          {
            "expr": "sum(ebpf_flow_duration)",
            "legendFormat": "Avg Duration",
            "refId": "C"
          }
        ],
        "yaxes": [
          {
            "format": "bytes",
            "label": "Bytes",
            "logBase": 1,
            "max": null,
            "min": 0
          },
          {
            "format": "short",
            "label": "Packets",
            "logBase": 1,
            "max": null,
            "min": 0
          }
        ],
        "options": {
          "tooltip": {
            "mode": "single"
          },
          "legend": {
            "showLegend": true
          }
        }
      },
      {
        "id": 8,
